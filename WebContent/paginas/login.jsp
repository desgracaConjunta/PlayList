<div class="loginForm">
	<form method="post" action="Handler">
		<div class="inputs">
			<input type="text" placeholder="Email" id="email" class="email">
		</div>
		<div class="inputs">
			<input type="password" placeholder="Password" id="pass" class="pass">
		</div>
		<div class="inputs">
			<input type="submit" value="Login" id="log" class="log">
		</div>
	</form>
</div>